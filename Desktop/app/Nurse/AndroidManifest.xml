<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.nurse"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".PatientInfo"
            android:label="@string/title_activity_patient_info" >
        </activity>
        <activity
            android:name=".ModifyVitalSign"
            android:label="@string/title_activity_modifysymptoms" >
        </activity>
        <activity
            android:name=".LogIn"
            android:label="@string/title_activity_log_in" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddPatient"
            android:label="@string/title_activity_add_patient" >
        </activity>
        <activity
            android:name=".SortUrgency"
            android:label="@string/title_activity_sort_urgency" >
        </activity>
    </application>

</manifest>
